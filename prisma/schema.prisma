generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  // description_ String   @map("description:")
  description  String
  imageUrl     String
  name         String
  price        Int
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("products")
}

